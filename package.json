{
  "name": "skylark-sidecomments",
  "description": "A version of side-comments that ported to running on skylarkjs ui.",
  "version": "0.9.0",
  "keywords": [
    "skylark",
    "integration",
    "side-comments"
  ],
  "homepage": "https://github.com/skylark-integration/skylark-sidecomments/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-integration/ .git"
  },
  "bugs": {
    "url": "https://github.com/skylark-integration/skylark-sidecomments/issues"
  },
  "files": [
    "demo/",
    "dist/",
    "src/",
    "skins/"
  ],
  "license": "MIT",
  "dependencies": {
    "skylark-jquery": "git+https://github.com/skylark-integration/skylark-jquery.git#master",
    "skylark-langx": "git+https://github.com/skylarklangx/skylark-langx.git#master"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "skylarkjs": {
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      }
    }
  }
}
